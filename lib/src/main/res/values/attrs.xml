<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidePager">
        <attr name="slide_progress_completed_fill_color" format="color" />
        <attr name="slide_progress_completed_reach_color" format="color" />
        <attr name="slide_progress_not_completed_reach_color" format="color" />
        <attr name="slide_progress_not_completed_outline_color" format="color" />
        <attr name="slide_progress_not_completed_outline_size" format="dimension" />
        <attr name="slide_progress_not_completed_future_outline_size" format="dimension" />
        <attr name="slide_progress_not_completed_fill_color" format="color" />
        <attr name="slide_progress_special_reach_color" format="color" />
        <attr name="slide_progress_special_outline_color" format="color" />
        <attr name="slide_progress_special_fill_color" format="color" />
        <attr name="slide_progress_reached_width" format="dimension" />
        <attr name="slide_start_at_end" format="boolean" />
        <attr name="slide_show_streaks" format="boolean" />
        <attr name="slide_show_progress_text" format="boolean" />
        <attr name="slide_show_week" format="boolean" />
        <attr name="slide_show_date" format="boolean" />
        <attr name="slide_progress_text_label" format="enum">
            <enum name="slide_progress_text_short" value="0"/>
            <enum name="slide_progress_text_long" value="1"/>
        </attr>

        <attr name="slide_progress_chart_bar_top_text_color" format="color"/>
        <attr name="slide_progress_chart_bar_bottom_text_color" format="color"/>
        <attr name="slide_progress_chart_bar_bottom_special_text_color" format="color" />
        <attr name="slide_progress_chart_color" format="color" />
    </declare-styleable>

</resources>